<table class="table table-hover" align="center">
  <thead>
    <tr>
      <td>图片</th>
      <td>名称</td>
      <td>价格</td>
      <td>状态</td>
      <td>操作</td>
    </tr>
  </thead>
  <tbody>
    <% menuRS.forEach(function(item){ %>
    <tr>
      <td><img src='<%-item.dishphoto%>' width='80' height='60'/></td>
      <td><%-item.dishname%></td>
      <td><%-item.price%></td>
      <td>
        <%
          if(item.putaway==1){
          %>
            在售
          <%
          }else{
          %>
            下架
          <%
          }
        %>
      </td>
      <td>
        <a href='#'>下架</a>&nbsp;
        <a href='#' onclick='updDish(this,<%-item.id%>)'>修改</a>&nbsp;
        <a href='#'>删除</a>
      </td>
    </tr>
    <tr id='t<%-item.id%>' style='display:none'>
      <td colspan="3" align="center">aaaaaaaaaa</td>
    </tr>
    <% }) %>
  </tbody>
</table>
